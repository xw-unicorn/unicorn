/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.flex-row{
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between
}
.flex-column{
    display: flex;
    flex-direction: column;
    align-items: center;
}

.text-xs{
  color: #bbb;
  font-size: 24rpx
}
.text-s{
  color: #bbb;
  font-size: 30rpx
}

.text-m{
  color: #515151;
  font-size: 36rpx
}
.panel{
  border-radius: 25rpx;
  margin-bottom: 20rpx
}